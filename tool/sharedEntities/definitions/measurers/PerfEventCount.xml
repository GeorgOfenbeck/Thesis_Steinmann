<?xml version="1.0" encoding="UTF-8"?>
<class
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="shared.xsd"
	name="PerfEventCount" 
	cBaseType="SharedEntityBase"
	javaBaseType=""
	javaSuffix="Data"
	comment="result of one observed event">
	
	<field name="definition" type="PerfEventDefinition" comment="definition of the counted event" />
	<field name="rawCount" type="ulong" comment="raw event count"/>
	<field name="timeEnabled" type="ulong" comment="time the event was enabled"/>
	<field name="timeRunning" type="ulong" comment="time the event was running"/>
	
</class>